create table DOOR_MECH_DEF (ID bigint generated by default as identity, NAME varchar (255) not null)

create table DOOR_COLOR (ID bigint generated by default as identity, NAME varchar (255) not null)

create table DOOR_COMPONENT (ID bigint generated by default as identity, NAME varchar (255) not null)

create table DOOR_MECH_TYPE (ID bigint generated by default as identity, NAME varchar (255) not null)
